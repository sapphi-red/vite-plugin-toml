{
  "name": "vite-plugin-toml",
  "version": "0.3.3",
  "description": "A plugin for vite to import a toml file.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm --target=node12",
    "lint": "eslint --ext .ts src",
    "format": "prettier --cache --write .",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphi-red/vite-plugin-toml.git"
  },
  "keywords": [
    "vite",
    "toml"
  ],
  "files": [
    "dist"
  ],
  "author": "sapphi-red",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphi-red/vite-plugin-toml/issues"
  },
  "homepage": "https://github.com/sapphi-red/vite-plugin-toml#readme",
  "peerDependencies": {
    "vite": "^2.6.14"
  },
  "devDependencies": {
    "@ltd/j-toml": "^1.24.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "tsup": "^5.7.0",
    "typescript": "^4.7.4",
    "vite": "^2.6.14"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1"
  }
}
