{
  "name": "vite-plugin-toml",
  "version": "0.7.0",
  "description": "A plugin for rollup/vite to import a toml file.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm --target=node14.17",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --cache .",
    "format": "prettier --cache --write .",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sapphi-red/vite-plugin-toml.git"
  },
  "keywords": [
    "vite",
    "toml"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  },
  "author": "sapphi-red",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sapphi-red/vite-plugin-toml/issues"
  },
  "homepage": "https://github.com/sapphi-red/vite-plugin-toml#readme",
  "peerDependencies": {
    "rollup": "^2.77.0 || ^3.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^22.16.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.6.2",
    "rollup": "^4.44.2",
    "tsup": "^7.3.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.2.0",
    "toml-eslint-parser": "^0.10.0"
  }
}
